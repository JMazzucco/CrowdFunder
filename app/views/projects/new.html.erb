<%= form_for(@project) do |f| %>

  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </div>
  <div class="field">
    <%= f.label :funding_goal %><br />
    <%= f.number_field :funding_goal %>
  </div>
  <div class="field">
    <%= f.label :start_date %><br />
    <%= f.date_field :start_date %>
  </div>
    <div class="field">
    <%= f.label :end_date %><br />
    <%= f.date_field :end_date %>
  </div>
    <div class="field">
    <%= f.label :photo %><br />
    <%= f.text_field :photo %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>